{
	"expo": {
		"name": "MuryoKani",
		"slug": "muryokani",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/icon.png",
				"backgroundColor": "#161616"
			},
			"jsEngine": "hermes",
			"softwareKeyboardLayoutMode": "pan"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#161616"
				}
			],
			"expo-sqlite",
			"expo-asset"
		],
		"splash": {
			"backgroundColor": "#161616",
			"image": "./assets/images/icon.png",
			"resizeMode": "contain"
		},
		"experiments": {
			"typedRoutes": true
		}
	}
}
